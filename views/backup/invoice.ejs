<body>
    <div id="invoiceContainer">
        <h1>INVOICE</h1>
        <div id="invoiceContent"></div>
        <table>
            <tr>
                <td>Full name</td>
                <td><%= data.full_name%></td>
            </tr>
            <tr>
                <td>Email</td>
                <td><%= data.email%></td>
            </tr>
            <tr>
                <td>Phone</td>
                <td><%= data.phone%></td>
            </tr>
            <tr>
